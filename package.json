{
  "name": "schummar-table",
  "repository": "schummar/schummar-table",
  "version": "0.10.3",
  "description": "",
  "type": "module",
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "exports": {
    ".": {
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/es/index.js"
    },
    "./mui5Theme": {
      "import": "./dist/es/mui5Theme.js",
      "require": "./dist/cjs/mui5Theme.js",
      "default": "./dist/es/mui5Theme.js"
    },
    "./mui4Theme": {
      "import": "./dist/es/mui4Theme.js",
      "require": "./dist/cjs/mui4Theme.js",
      "default": "./dist/es/mui4Theme.js"
    }
  },
  "types": "dist/types/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "dist/types/index.d.ts"
      ],
      "theme/mui5Theme": [
        "dist/types/theme/mui5Theme/index.d.ts"
      ],
      "theme/mui4Theme": [
        "dist/types/theme/mui4Theme/index.d.ts"
      ]
    }
  },
  "author": {
    "name": "Marco Schumacher",
    "email": "marco@schumacher.dev"
  },
  "license": "ISC",
  "scripts": {
    "start": "npm -w docs start",
    "build": "runp build:clean :p build:compile:* :s build:cjsPackage",
    "build:clean": "rimraf \"dist/**/*\"",
    "build:compile:js": "vite build",
    "build:compile:types": "tsc",
    "build:cjsPackage": "echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json",
    "lint": "runp lint:*",
    "lint:eslint": "eslint src",
    "lint:tsc": "tsc --emitDeclarationOnly false --noEmit",
    "prepublishOnly": "runp :s lint build",
    "docs:build": "npm -w docs run build"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "dayzed": "^3.2.3",
    "immer": "^9.0.15",
    "schummar-state": "0.4.3"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.12.3",
    "@mui/material": "^5.4.0",
    "@types/react": ">=17",
    "@types/react-dom": ">=17",
    "react": ">=17",
    "react-dom": ">=17"
  },
  "peerDependenciesMeta": {
    "@emotion/styled": {
      "optional": true
    },
    "@material-ui/core": {
      "optional": true
    },
    "@mui/material": {
      "optional": true
    }
  },
  "devDependencies": {
    "@emotion/styled": "^11.10.4",
    "@material-ui/core": "^4.12.4",
    "@mui/material": "^5.10.3",
    "@schummar/runp": "^1.5.3",
    "@types/react": "17",
    "@types/react-dom": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vitejs/plugin-react": "^2.0.1",
    "eslint": "^8.23.0",
    "eslint-plugin-storybook": "^0.6.4",
    "prettier": "^2.7.1",
    "react": "17",
    "react-dom": "17",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.5",
    "typescript": "^4.8.2",
    "vite": "^3.0.9"
  },
  "files": [
    "dist"
  ],
  "workspaces": [
    "docs"
  ],
  "volta": {
    "node": "16.17.0",
    "npm": "8.19.1"
  }
}
