{
  "name": "schummar-table",
  "repository": "schummar/schummar-table",
  "version": "0.4.0",
  "description": "",
  "type": "module",
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js"
  },
  "author": {
    "name": "Marco Schumacher",
    "email": "marco@schumacher.dev"
  },
  "license": "ISC",
  "scripts": {
    "start": "npm --prefix example run dev",
    "dev": "run-p dev:*",
    "dev:esm": "tsc --watch",
    "dev:cjs": "tsc -p tsconfig-cjs.json --watch",
    "build": "npm-run-all -s build:clean -p build:compile:*",
    "build:clean": "rimraf dist/**",
    "build:compile:esm": "tsc",
    "build:compile:cjs": "tsc -p tsconfig-cjs.json && echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint src",
    "lint:tsc": "tsc --noEmit",
    "prepublishOnly": "run-s test lint build",
    "test": "cd test && ava",
    "test-watch": "cd test && ava --watch",
    "coverage": "nyc --include src --all npm run test"
  },
  "peerDependencies": {
    "@material-ui/core": ">=4.11.4",
    "@material-ui/icons": ">=4.11.2",
    "@types/react": ">=17.0.11",
    "immer": ">=9.0.3",
    "react": ">=16.8.0",
    "schummar-state": ">=0.0.36"
  },
  "devDependencies": {
    "@testing-library/react": "^12.1.2",
    "@types/react": "*",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "ava": "^3.15.0",
    "eslint": "^8.2.0",
    "global-jsdom": "^8.3.0",
    "immer": "*",
    "jsdom": "^18.1.0",
    "localforage": "^1.10.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "schummar-state": "*",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "files": [
    "dist"
  ],
  "workspaces": [
    "example"
  ]
}
