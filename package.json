{
  "name": "schummar-table",
  "repository": "schummar/schummar-table",
  "version": "0.5.0",
  "description": "",
  "type": "module",
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "exports": {
    ".": {
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/es/index.js"
    },
    "./mui5Theme": {
      "import": "./dist/es/mui5Theme.js",
      "require": "./dist/cjs/mui5Theme.js",
      "default": "./dist/es/mui5Theme.js"
    },
    "./mui4Theme": {
      "import": "./dist/es/mui4Theme.js",
      "require": "./dist/cjs/mui4Theme.js",
      "default": "./dist/es/mui4Theme.js"
    }
  },
  "types": "dist/types/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "dist/types/index.d.ts"
      ],
      "theme/mui5Theme": [
        "dist/types/theme/mui5Theme/index.d.ts"
      ],
      "theme/mui4Theme": [
        "dist/types/theme/mui4Theme/index.d.ts"
      ]
    }
  },
  "author": {
    "name": "Marco Schumacher",
    "email": "marco@schumacher.dev"
  },
  "license": "ISC",
  "scripts": {
    "start": "vite",
    "build": "npm-run-all -s build:clean -p build:compile:* -s build:cjsPackage",
    "build:clean": "rimraf \"dist/**/*\"",
    "build:compile:js": "vite build",
    "build:compile:types": "tsc",
    "build:cjsPackage": "echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint src",
    "lint:tsc": "tsc --emitDeclarationOnly false --noEmit",
    "prepublishOnly": "run-s lint build",
    "docs": "npm -w docs start",
    "docs:build": "npm -w docs run build"
  },
  "dependencies": {
    "@emotion/react": "^11.8.1",
    "dayzed": "^3.2.2",
    "immer": "^9.0.12",
    "schummar-state": "0.2.1-experimental-13"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.12.3",
    "@mui/material": "^5.4.0",
    "@types/react": ">=17",
    "@types/react-dom": ">=17",
    "react": ">=17",
    "react-dom": ">=17"
  },
  "peerDependenciesMeta": {
    "@emotion/styled": {
      "optional": true
    },
    "@material-ui/core": {
      "optional": true
    },
    "@mui/material": {
      "optional": true
    }
  },
  "devDependencies": {
    "@emotion/styled": "^11.6.0",
    "@fontsource/fira-sans": "^4.5.2",
    "@material-ui/core": "^4.12.3",
    "@mui/material": "^5.4.1",
    "@types/react": "17",
    "@types/react-dom": "17",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "@vitejs/plugin-react": "^1.2.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "eslint": "^8.8.0",
    "localforage": "^1.10.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "react": "17",
    "react-dom": "17",
    "rimraf": "^3.0.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "vite": "^2.8.4",
    "zx": "^5.0.0"
  },
  "files": [
    "dist"
  ],
  "workspaces": [
    "example",
    "docs"
  ]
}
